{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "flashfire-frontend.vercel.app" }],
      "destination": "https://www.flashfirejobs.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "flashfire-frontend.vercel.app" }],
      "headers": [{ "key": "X-Robots-Tag", "value": "noindex, nofollow" }]
    }
  ]
}
